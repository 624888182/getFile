using System;

namespace FIH.ForeignStaff.db{
	///
	/// Summary description for PubNoticeInfo
	///
	public class PubNoticeInfo{
		private System.String _noticeCode;
		private System.String _title;
		private System.String _memo;
		private System.String _modifyUser;
		private System.DateTime? _modifyDate;
		private System.String _createUser;
		private System.DateTime? _createDate;
		
		public PubNoticeInfo(){
		}
		
		public PubNoticeInfo(
				System.String  noticeCode, 
				System.String  title, 
				System.String  memo, 
				System.String  modifyUser, 
				System.DateTime?  modifyDate, 
				System.String  createUser, 
				System.DateTime?  createDate 
			){
			this._noticeCode  = noticeCode;
			this._title  = title;
			this._memo  = memo;
			this._modifyUser  = modifyUser;
			this._modifyDate  = modifyDate;
			this._createUser  = createUser;
			this._createDate  = createDate;
		}
		
		public  System.String NoticeCode{
			get{ return _noticeCode; }
			set{ this._noticeCode = value; }
		}
		
		public  System.String Title{
			get{ return _title; }
			set{ this._title = value; }
		}
		
		public  System.String Memo{
			get{ return _memo; }
			set{ this._memo = value; }
		}
		
		public  System.String ModifyUser{
			get{ return _modifyUser; }
			set{ this._modifyUser = value; }
		}
		
		public  System.DateTime? ModifyDate{
			get{ return _modifyDate; }
			set{ this._modifyDate = value; }
		}
		
		public  System.String CreateUser{
			get{ return _createUser; }
			set{ this._createUser = value; }
		}
		
		public  System.DateTime? CreateDate{
			get{ return _createDate; }
			set{ this._createDate = value; }
		}
		
		
	}
}