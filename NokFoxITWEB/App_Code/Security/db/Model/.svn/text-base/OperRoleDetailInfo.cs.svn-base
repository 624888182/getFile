using System;

namespace FIH.Security.db{
	///
	/// Summary description for OperRoleDetailInfo
	///
	public class OperRoleDetailInfo{
		private System.String _operRoleGpCode;
		private System.String _moduleCode;
		private System.String _operCode;
		
		public OperRoleDetailInfo(){
		}
		
		public OperRoleDetailInfo(
				System.String  operRoleGpCode, 
				System.String  moduleCode, 
				System.String  operCode 
			){
			this._operRoleGpCode  = operRoleGpCode;
			this._moduleCode  = moduleCode;
			this._operCode  = operCode;
		}
		
		public  System.String OperRoleGpCode{
			get{ return _operRoleGpCode; }
			set{ this._operRoleGpCode = value; }
		}
		
		public  System.String ModuleCode{
			get{ return _moduleCode; }
			set{ this._moduleCode = value; }
		}
		
		public  System.String OperCode{
			get{ return _operCode; }
			set{ this._operCode = value; }
		}
		
		
	}
}