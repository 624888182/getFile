#brandMenu
{
    FONT-FAMILY: Arial; COLOR: #333333; FONT-SIZE: 12px;line-height:30px; width:150px; text-align:left; border-bottom:solid 1px #333333;  padding-left:10px;
    }
    #brandMenu a,A:hover,A:link{ text-decoration:none; color:#333333;}
    
.pName
{
    font:Arial;color:#333333;font-size:14px;line-height:30px; text-align:left; vertical-align:middle; padding-left:15px;
    }
    
.pDec
{
    font:Arial;color:#333333;font-size:12px;line-height:20px; text-align:left; vertical-align:middle; padding-left:15px; border-bottom:1px dotted #333333;
    }
    
    
.list 
{
     padding-left:10px; list-style-type:none; margin:0px;
    }
    
    .list  li{ display:inline; width:180px; margin:20px 10px 20px 10px; padding-top:15px;}
    .list li a,a:hover,a:link;a:visited{ color:#333333;font-size:12px; text-decoration:none;}
    .list li img{ width:150px; height:50px; border:0px; margin:auto;}
    
