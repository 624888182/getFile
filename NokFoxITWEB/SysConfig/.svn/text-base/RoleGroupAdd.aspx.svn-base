                    <asp:Button ID="btnExit" runat="server" CausesValidation="False"  SkinID="MainButton" OnClick="btnExit_Click" Text="退出" AccessKey="E" ToolTip="ALT+E" /></td>
            </tr>
            <tr >
                            <td colspan="4" class="TdMessage" >
                                 <asp:Label ID="lbMessage" runat="server" ForeColor="Red"></asp:Label></td>
                        </tr>
