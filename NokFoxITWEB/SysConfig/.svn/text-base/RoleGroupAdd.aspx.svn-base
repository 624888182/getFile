                            <td class="Tdleft" >
                                <asp:TextBox ID="tbRoleCode" runat="server" MaxLength="20" SkinID="NormalTextBox"  ></asp:TextBox></td>
                            <td></td>
                        </tr>
                        <tr>
                            <td class="TdRight" >
								<asp:label id="lbRoleName" runat="server" SkinID="RedLabel">權限組名稱 :</asp:label></td>
