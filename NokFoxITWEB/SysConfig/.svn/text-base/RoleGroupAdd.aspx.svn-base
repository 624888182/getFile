<%@ Page Language="C#" AutoEventWireup="true" CodeFile="RoleGroupAdd.aspx.cs" Inherits="SysConfig_RoleGroupAdd"  StylesheetTheme="SkinFile"%>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" >
<head runat="server">
    <title>无标题页</title>
<script language="javascript" type="text/javascript">
// <!CDATA[

function TABLE1_onclick() {

}

// ]]>
</script>
</head>
<body>
    <form id="form1" runat="server"> 
          <table border="0" cellpadding="0" cellspacing="0" width="95%" align="center" id="TABLE1" onclick="return TABLE1_onclick()" class="FounctionTable">
            <tr>
                <td class="headerLeft">
                    <asp:Label ID="lbHead" runat="server">權限組新增</asp:Label>
                    </td>
            </tr>
            
            
            <tr>
								<td colspan="1"   ></td>
							</tr>
            <tr>
                <td class="ButtonEareaLeft" colspan="1">
                    <asp:Button ID="btnReset" runat="server" CssClass="MainButton" OnClick="btnReset_Click" Text="重設" AccessKey="T" ToolTip="ALT+T" />
                    <asp:Button ID="btnAdd" runat="server" CssClass="MainButton" OnClick="btnAdd_Click" Text="新增" AccessKey="A" ToolTip="ALT+A" />
                    <asp:Button ID="btnModify" runat="server" CssClass="MainButton" OnClick="btnModify_Click" Text="修改" AccessKey="U" ToolTip="ALT+U" />
                    <asp:Button ID="btnDelete" runat="server" CssClass="MainButton" OnClick="btnDelete_Click" Text="刪除" AccessKey="D" ToolTip="ALT+D" />
